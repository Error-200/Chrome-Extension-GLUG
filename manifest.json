{  "manifest_version": 2,

  "name": "GNU/Linux User's Group",
  "description": "Chrome Extension to manage and keep track the events of GNU/Linux User's Group(NIT DGP)",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]

  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "browser_action":{
      "default_popup": "src/browser_action/browser_action.html"
  },
  "version": "1.0",
  "permissions":[
    "notifications"
  ]
}
